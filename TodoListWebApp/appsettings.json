{
  "AzureAd": {
    // Coordinates of the Azure AD Tenant
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "<directory>.onmicrosoft.com",
    "TenantId": "<tenent-id>",

    // Coordinates of the TechTalkWebApp
    "ClientId": "<web-app-client-id>",
    "CallbackPath": "/signin-oidc",
    "ClientSecret": "<web-app-client-secret>",

    // Coordinates of the TechTalkService
    "TechTalkResourceId": "api://<web-api-client-id>",
    "TechTalkBaseAddress": "https://localhost:44351"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  }
}
